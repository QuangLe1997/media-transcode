{
  "template_name": "faceswap_template",
  "description": "Face swap processing template with multiple output formats for different use cases",
  "version": "2.0",
  "profiles": [
    {
      "id_profile": "webp_preview_high_device",
      "input_type": "video",
      "output_filename": "preview_hd",
      "config": {
        "output_format": "webp",
        "width": 720,
        "height": null,
        "quality": 85,
        "fps": 12.0,
        "duration": 3.0,
        "start_time": 0,
        "speed": 1.0,
        "lossless": false,
        "animated": true,
        "loop": 0,
        "preset": "picture",
        "verbose": false
      }
    },
    {
      "id_profile": "webp_preview_low_device", 
      "input_type": "video",
      "output_filename": "preview_sd",
      "config": {
        "output_format": "webp",
        "width": 480,
        "height": null,
        "quality": 75,
        "fps": 8.0,
        "duration": 3.0,
        "start_time": 0,
        "speed": 1.0,
        "lossless": false,
        "animated": true,
        "loop": 0,
        "preset": "picture",
        "verbose": false
      }
    },
    {
      "id_profile": "mp4_detail_playback",
      "input_type": "video", 
      "output_filename": "detail_playback",
      "config": {
        "output_format": "mp4",
        "width": null,
        "height": null,
        "quality": 90,
        "codec": "h264",
        "crf": 20,
        "mp4_preset": "medium",
        "profile": "high",
        "level": "4.1",
        "pixel_format": "yuv420p",
        "audio_codec": "aac",
        "audio_bitrate": "128k",
        "audio_sample_rate": 44100,
        "two_pass": false,
        "hardware_accel": false,
        "verbose": false
      }
    },
    {
      "id_profile": "mp4_ai_processing",
      "input_type": "video",
      "output_filename": "ai_processing",
      "config": {
        "output_format": "mp4",
        "width": null,
        "height": null, 
        "quality": 95,
        "codec": "h264",
        "crf": 18,
        "mp4_preset": "slow",
        "profile": "high",
        "level": "4.1",
        "pixel_format": "yuv420p",
        "audio_codec": "aac",
        "audio_bitrate": "192k",
        "audio_sample_rate": 48000,
        "two_pass": true,
        "hardware_accel": false,
        "verbose": false
      }
    },
    {
      "id_profile": "webp_thumb_preview",
      "input_type": "image",
      "output_filename": "thumb_preview", 
      "config": {
        "output_format": "webp",
        "width": 400,
        "height": 400,
        "quality": 80,
        "lossless": false,
        "animated": false,
        "preset": "photo",
        "verbose": false
      }
    },
    {
      "id_profile": "jpg_detail_view",
      "input_type": "image",
      "output_filename": "detail_view",
      "config": {
        "output_format": "jpg", 
        "width": null,
        "height": null,
        "jpeg_quality": 90,
        "optimize": true,
        "progressive": false,
        "verbose": false
      }
    },
    {
      "id_profile": "png_ai_processing",
      "input_type": "image",
      "output_filename": "ai_processing",
      "config": {
        "output_format": "png",
        "width": null,
        "height": null,
        "quality": 100,
        "verbose": false
      }
    }
  ],
  "s3_output_config": {
    "enabled": true,
    "bucket": "your-bucket-name",
    "base_folder": "faceswap-outputs",
    "public_read": false,
    "upload_timeout": 900,
    "max_retries": 3
  },
  "face_detection_config": {
    "enabled": true,
    "confidence_threshold": 0.8,
    "extract_faces": true,
    "face_margin": 0.2,
    "output_format": "jpg",
    "output_quality": 95
  },
  "usage_examples": {
    "video_input": {
      "description": "Processing video input will generate 4 outputs",
      "outputs": [
        "preview_hd.webp - High quality WebP animation (720p, 3s, 12fps) for strong devices",
        "preview_sd.webp - Standard quality WebP animation (480p, 3s, 8fps) for weak devices", 
        "detail_playback.mp4 - Full quality MP4 with audio for detail viewing",
        "ai_processing.mp4 - Highest quality MP4 optimized for AI face swap processing"
      ]
    },
    "image_input": {
      "description": "Processing image input will generate 3 outputs", 
      "outputs": [
        "thumb_preview.webp - WebP thumbnail (400x400, Q80) for quick preview",
        "detail_view.jpg - Full quality JPEG (original size, Q90) for detail viewing",
        "ai_processing.png - Lossless PNG (original size) for AI face swap processing"
      ]
    }
  }
}