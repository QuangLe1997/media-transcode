  "sample_complete_requests": {
    "basic_video_processing": {
      "curl_command": "curl -X POST http://localhost:8087/transcode \\\n  -F 'source_url=https://example.com/video.mp4' \\\n  -F 'profiles=[{\"id_profile\":\"mobile_720p\",\"input_type\":\"video\",\"config\":{\"output_format\":\"mp4\",\"width\":1280,\"height\":720,\"codec\":\"h264\",\"crf\":23,\"mp4_preset\":\"fast\",\"audio_codec\":\"aac\",\"audio_bitrate\":\"128k\",\"hardware_accel\":true}}]' \\\n  -F 's3_output_config={\"base_path\":\"outputs\",\"folder_structure\":\"{task_id}/{profile_id}\",\"cleanup_temp_files\":true}'",
      "json_request": {
        "profiles": [
          {
            "id_profile": "mobile_720p",
            "input_type": "video",
            "config": {
              "output_format": "mp4",
              "width": 1280,
              "height": 720,
              "codec": "h264",
              "crf": 23,
              "mp4_preset": "fast",
              "audio_codec": "aac",
              "audio_bitrate": "128k",
              "hardware_accel": true
            }
          }
        ],
        "s3_output_config": {
          "base_path": "outputs",
          "folder_structure": "{task_id}/{profile_id}",
          "cleanup_temp_files": true
        }
      }
    },

    "multi_profile_processing": {
      "description": "Process video into multiple formats",
      "json_request": {
        "profiles": [
          {
            "id_profile": "video_1080p_high",
            "input_type": "video",
            "config": {
              "output_format": "mp4",
              "width": 1920,
              "height": 1080,
              "codec": "h264",
              "crf": 20,
              "mp4_preset": "medium",
              "audio_codec": "aac",
              "audio_bitrate": "256k"
            }
          },
          {
            "id_profile": "webp_preview_mobile",
            "input_type": "video", 
            "config": {
              "output_format": "webp",
              "width": 640,
              "height": 360,
              "quality": 75,
              "fps": 10,
              "duration": 3.0,
              "animated": true,
              "target_size": 200
            }
          },
          {
            "id_profile": "thumb_400px",
            "input_type": "video",
            "config": {
              "output_format": "webp",
              "width": 400,
              "height": 300,
              "quality": 85,
              "fps": 1,
              "duration": 0.1,
              "start_time": 2.0
            }
          }
        ],
        "s3_output_config": {
          "base_path": "multi-format-outputs",
          "folder_structure": "{task_id}/formats/{profile_id}",
          "cleanup_temp_files": true,
          "upload_timeout": 900
        }
      }
    },

    "image_batch_processing": {
      "description": "Process image into multiple sizes and formats",
      "json_request": {
        "profiles": [
          {
            "id_profile": "image_webp_optimized",
