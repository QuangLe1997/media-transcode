  "curl_examples": {
    "mobile_video_processing": "curl -X POST http://localhost:8087/transcode \\\n  -F 'source_url=https://example.com/video.mp4' \\\n  -F 'profiles=[\n    {\n      \"id_profile\": \"mobile_high_end_video\",\n      \"input_type\": \"video\",\n      \"config\": {\n        \"output_format\": \"mp4\",\n        \"width\": 1280,\n        \"height\": 720,\n        \"quality\": 85,\n        \"fps\": 30,\n        \"codec\": \"h264\",\n        \"crf\": 23,\n        \"mp4_preset\": \"medium\",\n        \"bitrate\": \"3M\",\n        \"profile\": \"main\",\n        \"audio_codec\": \"aac\",\n        \"audio_bitrate\": \"128k\",\n        \"hardware_accel\": true\n      }\n    }\n  ]' \\\n  -F 's3_output_config={\n    \"base_path\": \"mobile-outputs\",\n    \"folder_structure\": \"{task_id}/profiles/{profile_id}\",\n    \"cleanup_temp_files\": true\n  }'",

    "webp_animation_from_video": "curl -X POST http://localhost:8087/transcode \\\n  -F 'source_url=https://example.com/video.mp4' \\\n  -F 'profiles=[\n    {\n      \"id_profile\": \"webp_animated_hq\",\n      \"input_type\": \"video\",\n      \"config\": {\n        \"output_format\": \"webp\",\n        \"width\": 1280,\n        \"height\": 720,\n        \"quality\": 85,\n        \"fps\": 15,\n        \"duration\": 5.0,\n        \"start_time\": 1.0,\n        \"animated\": true,\n        \"method\": 4,\n        \"loop\": 0\n      }\n    }\n  ]' \\\n  -F 's3_output_config={\"base_path\": \"webp-outputs\"}'",

    "image_multi_size_processing": "curl -X POST http://localhost:8087/transcode \\\n  -F 'source_url=https://example.com/image.jpg' \\\n  -F 'profiles=[\n    {\n      \"id_profile\": \"image_webp_hq\",\n      \"input_type\": \"image\",\n      \"config\": {\n        \"output_format\": \"webp\",\n        \"width\": 1920,\n        \"height\": 1080,\n        \"quality\": 90,\n        \"method\": 6,\n        \"preset\": \"photo\"\n      }\n    },\n    {\n      \"id_profile\": \"thumbnail_medium\",\n      \"input_type\": \"image\",\n      \"config\": {\n        \"output_format\": \"webp\",\n        \"width\": 600,\n        \"height\": 400,\n        \"quality\": 85,\n        \"target_size\": 100\n      }\n    }\n  ]' \\\n  -F 's3_output_config={\"base_path\": \"image-processing\"}'"
