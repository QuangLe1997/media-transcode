  "parameter_reference": {
    "common_settings": {
      "width": "Optional[int] - Output width in pixels",
      "height": "Optional[int] - Output height in pixels", 
      "quality": "int (0-100) - General quality setting, default 85"
    },
    "video_timing_settings": {
      "fps": "Optional[float] - Output frame rate",
      "duration": "Optional[float] - Output duration in seconds",
      "start_time": "float - Start time in seconds, default 0",
      "speed": "float - Speed multiplier (0.5=slow, 2.0=fast), default 1.0"
    },
    "filter_settings": {
      "contrast": "float - Contrast adjustment, default 1.0",
      "brightness": "float - Brightness adjustment, default 0.0", 
      "saturation": "float - Saturation adjustment, default 1.0",
      "gamma": "float - Gamma adjustment, default 1.0",
      "enable_denoising": "bool - Enable noise reduction, default false",
      "enable_sharpening": "bool - Enable sharpening filter, default false",
      "auto_filter": "bool - Auto apply filters, default false"
    },
    "webp_specific_settings": {
      "lossless": "bool - WebP lossless mode, default false",
      "method": "int (0-6) - Compression method, higher=better compression, default 4",
      "preset": "str - WebP preset: default|photo|picture|drawing|icon|text",
      "near_lossless": "Optional[int] (0-100) - Near lossless quality",
      "alpha_quality": "int (0-100) - Alpha channel quality, default 100",
      "alpha_method": "int - Alpha compression method, default 1",
      "animated": "bool - Enable animation for video->WebP, default true",
      "loop": "int - Animation loop count (0=infinite), default 0",
      "pass_count": "int - Number of encoding passes, default 1",
      "target_size": "Optional[int] - Target file size in KB",
      "save_frames": "bool - Save individual frames, default false"
    },
    "jpeg_specific_settings": {
      "jpeg_quality": "int (0-100) - JPEG quality, default 90",
      "optimize": "bool - Enable JPEG optimization, default true", 
      "progressive": "bool - Progressive JPEG encoding, default false"
    },
    "mp4_specific_settings": {
      "codec": "str - Video codec: h264|h265, default h264",
      "crf": "int (0-51) - Constant Rate Factor, lower=better quality, default 23",
      "mp4_preset": "str - Encoding speed preset, default medium",
      "bitrate": "Optional[str] - Video bitrate (e.g., '2M', '5000k')",
      "max_bitrate": "Optional[str] - Maximum bitrate",
      "buffer_size": "Optional[str] - Buffer size for rate control",
      "profile": "str - H.264 profile: baseline|main|high, default high",
      "level": "str - H.264 level (e.g., '3.1', '4.1'), default 4.1",
      "pixel_format": "str - Pixel format, default yuv420p"
    },
    "audio_settings": {
      "audio_codec": "str - Audio codec: aac|mp3|none, default aac",
      "audio_bitrate": "str - Audio bitrate (e.g., '128k'), default 128k",
      "audio_sample_rate": "int - Sample rate in Hz, default 44100"
    },
    "system_settings": {
      "two_pass": "bool - Enable two-pass encoding, default false",
      "hardware_accel": "bool - Enable hardware acceleration, default false",
      "verbose": "bool - Enable verbose output, default true"
    }
  },

